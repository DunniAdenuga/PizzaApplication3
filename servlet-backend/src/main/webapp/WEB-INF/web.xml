<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">


    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.example.Dunni.myapplication.backend.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateCustomerServlet</servlet-name>
        <servlet-class>com.example.Dunni.myapplication.backend.CreateCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateCustomerServlet</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateOrdersServlet</servlet-name>
        <servlet-class>com.example.Dunni.myapplication.backend.CreateOrdersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateOrdersServlet</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadCustomerServlet</servlet-name>
        <servlet-class>com.example.Dunni.myapplication.backend.ReadCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadCustomerServlet</servlet-name>
        <url-pattern>/customerRead</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadOrdersServlet</servlet-name>
        <servlet-class>com.example.Dunni.myapplication.backend.ReadOrdersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadOrdersServlet</servlet-name>
        <url-pattern>/orderRead</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>